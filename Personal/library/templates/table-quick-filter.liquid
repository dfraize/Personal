{% block title %}{{ model.title }} : {{ global.site_title }}{% endblock %}
{% block content %}
<section class="col-24">
	<h1>{{ model.title }}</h1>	
</section>

<section class="col-24">
	<section class="col-8 first">

		<div class="js-mul-tabs-example mul-tabs">
			{% include "page-tabs" %}
			<div id="tabs-1">
				<p>Description go here&hellip;</p>
			</div>
			<div id="tabs-2">
				<p>When to use details go here&hellip;</p>
			</div>
			<div id="tabs-3">
				<p>Functional details go here&hellip;</p>
			</div>
			<div id="tabs-4">
				<p>Technical details go here&hellip;</p>
			</div>
		</div>

		<div class="col-24">
			<h4 class="mul-hdr-rule">Dependencies</h4>
			<table class="mul-table mul-table-borders" id="" border="0" cellpadding="0" cellspacing="0" title="" summary="">
				<thead>
					<tr>
						<th width="" scope="col">Library</th>
						<th width="" scope="col">Location or Download</th>
					</tr>
				</thead>
				<tbody>
					<tr>
						<td>jQuery 1.10.2</td>
						<td>assets/scripts/core_v2/lib/jquery</td>
					</tr>
					<tr>
						<td>FooTables 2.0</td>
						<td>assets/scripts/core_v2/lib/jquery/plugins</td>
					</tr>
					<tr>
						<td>FooTables 2.0 - Filter Plugin</td>
						<td>assets/scripts/core_v2/lib/jquery/plugins</td>
					</tr>
				</tbody>
			</table>
		</div>

	</section>
	<section class="col-16">
		<h4 class="mul-hdr-rule">Example</h4>

		<!-- [INSERT UI COMPONENT NAME] - EXAMPLE - BEGINS -->
		<table class="mul-table mul-footable mul-table-borders mul-table-striped mul-table-hover" data-filter="#filter" data-filter-timeout="300" data-filter-text-only="true" data-filter-minimum="1" id="" border="0" cellpadding="0" cellspacing="0" title="" summary="">
			<caption>
				<div class="col-19 first">&nbsp;</div>
				<div class="col-5 mul-tbl-data-filter">
					<input id="filter" type="text" placeholder="Filter" tabindex="" title="Type to filter results." />
				</div>
			</caption>
			<thead>
				<tr>
					<th class="col-1" scope="col"></th>
					<th class="col-6" scope="col"><a href="javascript:;">Task</a></th>
					<th data-sort-initial="ascending" class="col-2" scope="col"><a href="javascript:;">Type</a></th>
					<th class="col-8" scope="col">
						<a href="javascript:;">Client</a> |
						<a href="javascript:;">Location</a>
					</th>
					<th class="col-4" scope="col"><a href="javascript:;">Assigned To</a></th>
					<th class="col-3 mul-nowrap mul-align-right" scope="col"><a href="javascript:;">Start Date</a></th>
					<th class="col-3 mul-nowrap mul-align-right" scope="col"><a href="javascript:;">End Date</a></th>
				</tr>
			</thead>
			<tbody>

				{% for item in global.table_data %}
				<tr>
					<td>{{ item.checkbox }}</td>
					<td>{{ item.task }}</td>
					<td>{{ item.type_global }}</td>
					<td>
						<a href="javascript:;">{{ item.company }}</a> |
						{{ item.location}}
					</td>
					<td>
						{{ item.abbrev_first_name}} {{ item.last_name}}
					</td>
					<td class="mul-nowrap mul-align-right">{{ item.start_date }}</td>
					<td class="mul-nowrap mul-align-right">{{ item.end_date }}</td>
				</tr>
				{% endfor %}
				
			</tbody>
		</table>
		<!-- [INSERT UI COMPONENT NAME] - EXAMPLE - ENDS -->

		<!-- GET CODE - BEGINS
		<div class="col-24">
			<div class="col-24 get-code-btn-ctn">
				{% include "get-code-button" %}
			</div>

			<div class="col-24 js-mul-toggle-content mul-display-none" id="div1">

				<h4 class="mul-hdr-rule">HTML</h4>
				<p>Put any content you want in here.</p>

<pre class="prettyprint linenums">

</pre>

				<h4 class="mul-hdr-rule">JS</h4>
				<p>Put any content you want in here.</p>

<pre class="prettyprint linenums">

</pre>

			</div>

		</div>
		GET CODE - ENDS -->

	</section>
</section>
{% endblock %}