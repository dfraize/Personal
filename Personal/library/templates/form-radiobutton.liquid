{% block title %}{{ model.title }} : {{ global.site_title }}{% endblock %}
{% block content %}
<section class="col-24">
	<h1>{{ model.title }}</h1>	
</section>

<section class="col-24">
	<section class="col-8 first">

		<div class="js-mul-tabs-example mul-tabs">
			{% include "page-tabs" %}
			<div id="tabs-1">
				<p>Description go here&hellip;</p>
			</div>
			<div id="tabs-2">
				<p>When to use details go here&hellip;</p>
			</div>
			<div id="tabs-3">
				<p>Functional details go here&hellip;</p>
			</div>
			<div id="tabs-4">
				<p>Technical details go here&hellip;</p>
			</div>
		</div>

	</section>
	<section class="col-16">

		<h4 class="mul-hdr-rule">Default Layout</h4>
		<div class="col-24">
			<div class="col-6 first">

			<!-- DEFAULT EXAMPLE - BEGINS -->
				<form class="mul-form" id="" name="" method="" action="" title="">
					<fieldset>
						<ol>
							<li>
								<label for="">Label Posed as a Question?</label>
								<div class="mul-radio">
									<label for="question1" class="mul-form-inline-element">
										<input name="question" type="radio" id="question1"> Yes
									</label>

									<label for="question2" class="mul-form-inline-element">
										<input name="question" type="radio" id="question2"> No
									</label>
								</div>
							</li>

							<li class="mul-form-action"></li>
						</ol>
					</fieldset>
				</form>
			<!-- DEFAULT EXAMPLE - ENDS -->

			</div>
		</div>

		<h4 class="mul-hdr-rule">In-Line Layout</h4>
		<div class="col-24">
			<div class="col-14 first">

			<!-- DEFAULT EXAMPLE - BEGINS -->
				<form class="mul-for mul-form-inline" id="" name="" method="" action="" title="">
					<fieldset>
						<ol>
							<li>
								<label class="col-10 first" for="">Label Posed as a Question?</label>
								<div class="mul-radio col-14">
									<label for="question1" class="mul-form-inline-element">
										<input name="question" type="radio" id="question1"> Yes
									</label>

									<label for="question2" class="mul-form-inline-element">
										<input name="question" type="radio" id="question2"> No
									</label>
								</div>
							</li>
							
							<li class="mul-form-action"></li>
						</ol>
					</fieldset>
				</form>
			<!-- DEFAULT EXAMPLE - ENDS -->

			</div>
		</div>

		<!-- GET CODE - BEGINS
		<div class="col-24">
			<div class="col-24 get-code-btn-ctn">
				{% include "get-code-button" %}
			</div>

			<div class="col-24 js-mul-toggle-content mul-display-none" id="div1">

				<h4 class="mul-hdr-rule">HTML</h4>
				<p>Put any content you want in here.</p>

<pre class="prettyprint linenums">

</pre>

			</div>

		</div>
		GET CODE - ENDS -->

	</section>
</section>
{% endblock %}